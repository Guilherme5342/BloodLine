{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build make file",
            "type": "shell",
            "command": "./premake5.exe",
            "args": ["gmake2"],
            "group": "build"
        },
        {
            "label": "Build Debug",
            "type": "shell",
            "command": "mingw32-make",

            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": "Build make file"
        },
        {
            "label": "Build Release",
            "type": "shell",
            "command": "mingw32-make config=release",
            "group": "build",
            "dependsOn": "Build make file"
        }
    ]
}
