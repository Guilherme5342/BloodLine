{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/Bloodline/src",
            "${workspaceFolder}/GalapagosEngine-Core/include"
        ],
        "myDefines": ["DEBUG", "MY_FEATURE=1"]
    },
    "configurations": [
        {
            "name": "win32gcc",
            "includePath": ["${myIncludePath}", "C:/SDL32/include"],
            "defines": ["_DEBUG", "DEBUG", "UNICODE", "_UNICODE"],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "C:/TDM-GCC-32/bin/g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-x86",
            "compilerArgs": ["-Wall", "-Wextra", "-O0"]
        },
        {
            "name": "linux64gcc",
            "includePath": ["${myIncludePath}", "/usr/include"],
            "defines": ["_DEBUG", "DEBUG", "UNICODE", "_UNICODE"],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": ["-Wall", "-Wextra", "-O0"]
        },
        {
            "name": "macos32gcc",
            "includePath": ["${myIncludePath}", "/usr/include"],
            "defines": ["_DEBUG", "DEBUG", "UNICODE", "_UNICODE"],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-gcc-x86",
            "compilerArgs": ["-Wall", "-Wextra", "-O0"]
        }
    ],
    "version": 4
}
